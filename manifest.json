{
  "name": "Podoro Timer",
  "version": "0.9.5",
  "manifest_version": 3,
  "description":  "A pomodoro timer built into your browser.",
  "permissions": [
    "notifications",
    "alarms",
    "storage"
  ],
  "icons": {
    "16":"icons/red_icon_16.png",
    "32":"icons/red_icon_32.png",
    "64":"icons/red_icon_64.png",
    "128":"icons/red_icon_128.png"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16":"icons/red_icon_16.png",
      "32":"icons/red_icon_32.png",
      "64":"icons/red_icon_64.png",
      "128":"icons/red_icon_128.png"
    }
  },
  "background": {
    "service_worker": "/scripts/background.js",
    "type": "module"
  }
}